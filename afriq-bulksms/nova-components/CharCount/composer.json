{
    "name": "afriq/char-count",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Afriq\\CharCount\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Afriq\\CharCount\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
